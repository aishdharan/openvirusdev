      <mo>)</mo>
     </mrow>
     <mrow>
      <mi>N</mi>
     </mrow>
    </mfrac>
    <mo>×</mo>
    <mi mathvariant="normal">Δ</mi>
    <mi>t</mi>
    <mo>−</mo>
    <mi>i</mi>
    <mo>(</mo>
    <mrow>
     <mi>t</mi>
    </mrow>
    <mo>)</mo>
    <mo>×</mo>
    <mi>μ</mi>
    <mo>×</mo>
