    <mo>(</mo>
    <mrow>
     <mi>t</mi>
     <mo>+</mo>
     <mi mathvariant="normal">Δ</mi>
     <mi>t</mi>
    </mrow>
    <mo>)</mo>
    <mo>−</mo>
    <mi>i</mi>
    <mo>(</mo>
    <mrow>
     <mi>t</mi>
    </mrow>
    <mo>)</mo>
    <mo>=</mo>
    <mi>i</mi>
    <mo>(</mo>
    <mrow>
