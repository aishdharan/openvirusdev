             <mi>n</mi>
             <mo>+</mo>
             <mn>1</mn>
            </mrow>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo>,</mo>
          <mspace width="1em"/>
          <mo>∀</mo>
          <mi>n</mi>
          <mo>∈</mo>
          <msub>
           <mover>
            <mrow>
             <mi>I</mi>
            </mrow>
            <mo stretchy="true">¯</mo>
           </mover>
           <mrow>
            <mi>N</mi>
            <mo>−</mo>
            <mn>1</mn>
           </mrow>
          </msub>
          <mo>,</mo>
         </mrow>
        </mtd>
       </mtr>
       <mtr>
        <mtd/>
       </mtr>
       <mtr>
        <mtd columnalign="left">
         <mrow>
          <msub>
           <mi>L</mi>
           <mi>R</mi>
          </msub>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mi>n</mi>
           </msub>
           <mo>)</mo>
          </mrow>
         </mrow>
        </mtd>
        <mtd columnalign="left">
         <mrow>
          <mo>=</mo>
          <msub>
           <mi>δ</mi>
           <mi>t</mi>
          </msub>
          <mi>R</mi>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mi>n</mi>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo linebreak="badbreak">−</mo>
          <mi>κ</mi>
          <mi>E</mi>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mi>n</mi>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo linebreak="badbreak">−</mo>
          <mi>r</mi>
          <mi>I</mi>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mi>n</mi>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo linebreak="badbreak">−</mo>
          <mi>q</mi>
          <mi>Q</mi>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mi>n</mi>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo linebreak="badbreak">+</mo>
          <mi>μ</mi>
          <mi>R</mi>
          <mrow>
           <mo>(</mo>
           <msub>
            <mi>t</mi>
            <mrow>
             <mi>n</mi>
             <mo>+</mo>
             <mn>1</mn>
            </mrow>
           </msub>
           <mo>)</mo>
          </mrow>
          <mo>,</mo>
          <mspace width="1em"/>
          <mo>∀</mo>
          <mi>n</mi>
          <mo>∈</mo>
          <msub>
           <mover>
            <mrow>
             <mi>I</mi>
            </mrow>
            <mo stretchy="true">¯</mo>
           </mover>
           <mrow>
            <mi>N</mi>
            <mo>−</mo>
            <mn>1</mn>
           </mrow>
          </msub>
          <mo>.</mo>
         </mrow>
        </mtd>
       </mtr>
      </mtable>
     </mrow>
    </math>
   </disp-formula>Here, we used the discrete operator
   <disp-formula id="eq0036">
    <label>(3.11)</label>
    <math id="M88" altimg="si92.svg">
     <mrow>
      <msub>
       <mi>δ</mi>
       <mi>t</mi>
      </msub>
      <mi>U</mi>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>t</mi>
        <mi>n</mi>
       </msub>
       <mo>)</mo>
      </mrow>
      <mo linebreak="goodbreak">=</mo>
      <mfrac>
       <mrow>
        <mi>U</mi>
        <mrow>
         <mo>(</mo>
         <msub>
          <mi>t</mi>
          <mrow>
           <mi>n</mi>
           <mo>+</mo>
           <mn>1</mn>
          </mrow>
         </msub>
         <mo>)</mo>
        </mrow>
        <mo>−</mo>
        <mi>U</mi>
        <mrow>
         <mo>(</mo>
         <msub>
          <mi>t</mi>
          <mi>n</mi>
         </msub>
         <mo>)</mo>
        </mrow>
       </mrow>
       <mi>τ</mi>
      </mfrac>
      <mo>,</mo>
      <mo>∀</mo>
      <mi>t</mi>
      <mo>∈</mo>
      <mrow>
       <mo>[</mo>
       <mn>0</mn>
       <mo>,</mo>
       <mi>T</mi>
       <mo>]</mo>
      </mrow>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>where 
   <italic>U</italic> can be any of the functions 
   <italic>S, E, I, Q</italic> or 
   <italic>R</italic> for the remainder of this proof. Notice that, under the regularity assumptions and using the classical argument based on Taylor’s theorem, it follows that there exist constants 
   <inline-formula>
    <math id="M89" altimg="si93.svg">
     <msubsup>
      <mi>C</mi>
      <mi>U</mi>
      <mn>1</mn>
     </msubsup>
    </math>
   </inline-formula> and 
   <inline-formula>
    <math id="M90" altimg="si94.svg">
     <msubsup>
      <mi>C</mi>
      <mi>U</mi>
      <mn>2</mn>
     </msubsup>
    </math>
   </inline-formula> which are independent of 
   <italic>τ</italic>, such that
   <disp-formula id="eq0037">
    <label>(3.12)</label>
    <math id="M91" altimg="si95.svg">
     <mrow>
      <mrow>
       <mo>|</mo>
      </mrow>
      <msup>
       <mi>U</mi>
       <mo>′</mo>
      </msup>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>t</mi>
        <mi>n</mi>
       </msub>
       <mo>)</mo>
      </mrow>
      <mo linebreak="goodbreak">−</mo>
      <msub>
       <mi>δ</mi>
       <mi>t</mi>
      </msub>
      <mi>U</mi>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>t</mi>
        <mi>n</mi>
       </msub>
       <mo>)</mo>
      </mrow>
      <mrow>
       <mo>|</mo>
       <mo>≤</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>U</mi>
       <mn>1</mn>
      </msubsup>
      <mi>τ</mi>
      <mo>,</mo>
      <mspace width="1em"/>
      <mo>∀</mo>
      <mi>n</mi>
      <mo>∈</mo>
      <msub>
       <mover>
        <mrow>
         <mi>I</mi>
        </mrow>
        <mo stretchy="true">¯</mo>
       </mover>
       <mrow>
        <mi>N</mi>
        <mo>−</mo>
        <mn>1</mn>
       </mrow>
      </msub>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>
   <disp-formula id="eq0038">
    <label>(3.13)</label>
    <math id="M92" altimg="si96.svg">
     <mrow>
      <mrow>
       <mo>|</mo>
       <mi>U</mi>
      </mrow>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>t</mi>
        <mi>n</mi>
       </msub>
       <mo>)</mo>
      </mrow>
      <mo linebreak="goodbreak">−</mo>
      <mi>U</mi>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>t</mi>
        <mrow>
         <mi>n</mi>
         <mo>+</mo>
         <mn>1</mn>
        </mrow>
       </msub>
       <mo>)</mo>
      </mrow>
      <mrow>
       <mo>|</mo>
       <mo>≤</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>U</mi>
       <mn>2</mn>
      </msubsup>
      <mi>τ</mi>
      <mo>,</mo>
      <mspace width="1em"/>
      <mo>∀</mo>
      <mi>n</mi>
      <mo>∈</mo>
      <msub>
       <mover>
        <mrow>
         <mi>I</mi>
        </mrow>
        <mo stretchy="true">¯</mo>
       </mover>
       <mrow>
        <mi>N</mi>
        <mo>−</mo>
        <mn>1</mn>
       </mrow>
      </msub>
      <mo>.</mo>
     </mrow>
    </math>
   </disp-formula>Using the smoothness of 
   <italic>E</italic> and 
   <italic>I</italic>, we can fix respective bounds 
   <inline-formula>
    <math id="M93" altimg="si97.svg">
     <msubsup>
      <mi>C</mi>
      <mi>E</mi>
      <mn>0</mn>
     </msubsup>
    </math>
   </inline-formula> and 
   <inline-formula>
    <math id="M94" altimg="si98.svg">
     <msubsup>
      <mi>C</mi>
      <mi>I</mi>
      <mn>0</mn>
     </msubsup>
    </math>
   </inline-formula> of these functions on [0, 
   <italic>T</italic>]. Using the triangle ineqlauity and the bounds above, we observe then that there exists a constant 
   <italic>C
    <sub>U</sub>
   </italic> which is independent of 
   <italic>τ</italic>, such that the following are satisfied for 
   <italic>U</italic> being any of 
   <italic>S, E, I, Q</italic> or 
   <italic>R</italic>:
   <disp-formula id="eq0039">
    <label>(3.14)</label>
    <math id="M95" altimg="si99.svg">
     <mrow>
      <mrow>
       <mo stretchy="true">|</mo>
       <msub>
        <mi mathvariant="bold-script">L</mi>
        <mi>U</mi>
       </msub>
       <mrow>
        <mo>(</mo>
        <msub>
         <mi>t</mi>
         <mi>n</mi>
        </msub>
        <mo>)</mo>
       </mrow>
       <mo linebreak="badbreak">−</mo>
       <msub>
        <mi>L</mi>
        <mi>U</mi>
       </msub>
       <mrow>
        <mo>(</mo>
        <msub>
         <mi>t</mi>
         <mi>n</mi>
        </msub>
        <mo>)</mo>
       </mrow>
       <mo stretchy="true">|</mo>
      </mrow>
      <mo>≤</mo>
      <msub>
       <mi>C</mi>
       <mi>S</mi>
      </msub>
      <mi>τ</mi>
      <mo>,</mo>
      <mspace width="1em"/>
      <mo>∀</mo>
      <mi>n</mi>
      <mo>∈</mo>
      <msub>
       <mover>
        <mrow>
         <mi>I</mi>
        </mrow>
        <mo stretchy="true">¯</mo>
       </mover>
       <mrow>
        <mi>N</mi>
        <mo>−</mo>
        <mn>1</mn>
       </mrow>
      </msub>
      <mo>.</mo>
     </mrow>
    </math>
   </disp-formula>To be more precise, those constants are
   <disp-formula id="eq0040">
    <label>(3.15)</label>
    <math id="M96" altimg="si100.svg">
     <mrow>
      <msub>
       <mi>C</mi>
       <mi>S</mi>
      </msub>
      <mo linebreak="goodbreak">=</mo>
      <msubsup>
       <mi>C</mi>
       <mi>S</mi>
       <mn>1</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>β</mi>
        <mn>1</mn>
       </msub>
       <msubsup>
        <mi>C</mi>
        <mi>I</mi>
        <mn>0</mn>
       </msubsup>
       <mo linebreak="badbreak">+</mo>
       <msub>
        <mi>β</mi>
        <mn>2</mn>
       </msub>
       <msubsup>
        <mi>C</mi>
        <mi>E</mi>
        <mn>0</mn>
       </msubsup>
       <mo>)</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>S</mi>
       <mn>2</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mi>μ</mi>
      <msubsup>
       <mi>C</mi>
       <mi>S</mi>
       <mn>2</mn>
      </msubsup>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>
   <disp-formula id="eq0041">
    <label>(3.16)</label>
    <math id="M97" altimg="si101.svg">
     <mrow>
      <msub>
       <mi>C</mi>
       <mi>E</mi>
      </msub>
      <mo linebreak="goodbreak">=</mo>
      <msubsup>
       <mi>C</mi>
       <mi>E</mi>
       <mn>1</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mrow>
       <mo>(</mo>
       <msub>
        <mi>q</mi>
        <mn>1</mn>
       </msub>
       <mo linebreak="badbreak">+</mo>
       <mi>κ</mi>
       <mo linebreak="badbreak">+</mo>
       <mi>α</mi>
       <mo linebreak="badbreak">+</mo>
       <mi>μ</mi>
       <mo>)</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>E</mi>
       <mn>2</mn>
      </msubsup>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>
   <disp-formula id="eq0042">
    <label>(3.17)</label>
    <math id="M98" altimg="si102.svg">
     <mrow>
      <msub>
       <mi>C</mi>
       <mi>I</mi>
      </msub>
      <mo linebreak="goodbreak">=</mo>
      <msubsup>
       <mi>C</mi>
       <mi>I</mi>
       <mn>1</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mrow>
       <mo>(</mo>
       <mi>r</mi>
       <mo linebreak="badbreak">+</mo>
       <mi>μ</mi>
       <mo linebreak="badbreak">+</mo>
       <msub>
        <mi>d</mi>
        <mn>1</mn>
       </msub>
       <mo>)</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>I</mi>
       <mn>2</mn>
      </msubsup>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>
   <disp-formula id="eq0043">
    <label>(3.18)</label>
    <math id="M99" altimg="si103.svg">
     <mrow>
      <msub>
       <mi>C</mi>
       <mi>Q</mi>
      </msub>
      <mo linebreak="goodbreak">=</mo>
      <msubsup>
       <mi>C</mi>
       <mi>Q</mi>
       <mn>1</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mrow>
       <mo>(</mo>
       <mi>q</mi>
       <mo linebreak="badbreak">+</mo>
       <mi>μ</mi>
       <mo linebreak="badbreak">+</mo>
       <msub>
        <mi>d</mi>
        <mn>2</mn>
       </msub>
       <mo>)</mo>
      </mrow>
      <msubsup>
       <mi>C</mi>
       <mi>Q</mi>
       <mn>2</mn>
      </msubsup>
      <mo>,</mo>
     </mrow>
    </math>
   </disp-formula>
   <disp-formula id="eq0044">
    <label>(3.19)</label>
    <math id="M100" altimg="si104.svg">
     <mrow>
      <msub>
       <mi>C</mi>
       <mi>R</mi>
      </msub>
      <mo linebreak="goodbreak">=</mo>
      <msubsup>
       <mi>C</mi>
       <mi>R</mi>
       <mn>1</mn>
      </msubsup>
      <mo linebreak="goodbreak">+</mo>
      <mi>μ</mi>
      <msubsup>
       <mi>C</mi>
       <mi>R</mi>
       <mn>2</mn>
      </msubsup>
      <mo>.</mo>
     </mrow>
    </math>
   </disp-formula>We conclude that 
   <xref rid="eq0027" ref-type="disp-formula">(3.2)</xref> yields first-order approximations to the solutions of 
   <xref rid="eq0001" ref-type="disp-formula">(2.1)</xref>, as desired. □
  </p>
 </statement>
</p>
