     </mrow>
    </msub>
    <mo>(</mo>
    <mrow>
     <mi>t</mi>
    </mrow>
    <mo>)</mo>
    <mo>=</mo>
    <mfrac>
     <mrow>
      <mn>1</mn>
     </mrow>
     <mrow>
      <msub>
       <mrow>
        <mn>2</mn>
        <mi>π</mi>
        <mi>σ</mi>
       </mrow>
       <mrow>
        <mi>k</mi>
       </mrow>
      </msub>
     </mrow>
    </mfrac>
    <mspace width="2pt"/>
    <mi>e</mi>
    <mi>x</mi>
    <mi>p</mi>
    <mo>(</mo>
    <mrow>
     <mfrac>
      <mrow>
       <mn>1</mn>
       <msup>
        <mrow>
         <mo>(</mo>
         <mrow>
          <msub>
           <mrow>
            <mi>q</mi>
           </mrow>
           <mrow>
            <mi>k</mi>
           </mrow>
          </msub>
          <mo>-</mo>
          <mi>t</mi>
         </mrow>
         <mo>)</mo>
        </mrow>
        <mrow>
         <mn>2</mn>
        </mrow>
       </msup>
      </mrow>
      <mrow>
       <msubsup>
        <mrow>
         <mn>2</mn>
         <mi>σ</mi>
        </mrow>
        <mrow>
         <mi>k</mi>
        </mrow>
        <mrow>
         <mn>2</mn>
        </mrow>
       </msubsup>
      </mrow>
     </mfrac>
    </mrow>
    <mo>)</mo>
   </math>
  </alternatives>
