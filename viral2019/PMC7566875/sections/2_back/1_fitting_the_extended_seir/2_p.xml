       </mrow>
       <mrow>
        <mn>1</mn>
       </mrow>
      </msub>
      <mo>,</mo>
      <msub>
       <mrow>
        <mi>β</mi>
       </mrow>
       <mrow>
        <mn>2</mn>
       </mrow>
      </msub>
      <mo>,</mo>
      <mi mathvariant="bold-fraktur">f</mi>
      <mo>,</mo>
      <mi mathvariant="bold-fraktur">g</mi>
      <mo>,</mo>
      <msub>
       <mrow>
        <mi>τ</mi>
       </mrow>
       <mrow>
        <mi mathvariant="bold-fraktur">E</mi>
       </mrow>
      </msub>
      <mo>,</mo>
      <msub>
       <mrow>
        <mi>τ</mi>
       </mrow>
       <mrow>
        <mi mathvariant="bold-fraktur">I</mi>
        <mi mathvariant="bold-fraktur">P</mi>
       </mrow>
      </msub>
      <mo>,</mo>
     </mrow>
    </mfenced>
    <mfenced close=")">
     <mrow>
